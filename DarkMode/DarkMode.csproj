<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <LangVersion>latest</LangVersion>
        <DebugType>embedded</DebugType>

        <VersionPrefix>1.1.5</VersionPrefix>
        <Description>DarkMode For Chat</Description>
        <Authors>Techiee</Authors>
    </PropertyGroup>

    <ItemGroup>
      <EmbeddedResource Include="ImageResource\AmogusBan.png" />
      <EmbeddedResource Include="ImageResource\DarkKeyboard.png" />
      <EmbeddedResource Include="ImageResource\DarkQuickChat.png" />
